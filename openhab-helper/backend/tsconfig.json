  "compilerOptions": {
    "sourceMap": true,
    "outDir": "../dist/backend",
    "importHelpers": true,
    "removeComments": true,
    "skipLibCheck": true,
    "target": "ES2018",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true
  }
}
